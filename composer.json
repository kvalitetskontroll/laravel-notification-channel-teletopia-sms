{
    "name": "kvalitetskontroll/laravel-notification-channel-teletopia-sms",
    "description": "TeletopiaSMS Notification Channel For Laravel",
    "homepage": "https://github.com/kvalitetskontroll/laravel-notification-channel-teletopia-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Kvalitetskontroll AS",
            "email": "it@kvalitetskontroll.no",
            "homepage": "https://www.kvalitetskontroll.no/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kvalitetskontroll\\TeletopiaSMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kvalitetskontroll\\TeletopiaSMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
